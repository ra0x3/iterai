    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prettier": "^3.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
